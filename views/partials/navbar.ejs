<!-- views/partials/navbar.ejs -->
<nav>
    <ul>
      <!-- Link to home page -->
      <li><a href="/">Home</a></li>
  
      <!-- Conditional rendering based on user authentication status -->
      <% if (user) { %>
        <!-- Link to user's car collection, dynamically using user ID -->
        <li><a href="/cars/collections">Your Car Collection</a></li>
        <li><a href="/cars">All Cars</a></li>
  
        <!-- Display sign out link -->
        <li><a href="/auth/sign-out">Sign Out</a></li>
      <% } else { %>
        <!-- Display sign in and sign up links -->
        <li><a href="/auth/sign-in">Sign In</a></li>
        <li><a href="/auth/sign-up">Sign Up</a></li>
      <% } %>
    </ul>
  </nav>
  